{
    "manifest_version": 2,
    "name": "The YT Note Taker",
    "version": "0.1",
 
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
        "matches": [
          "https://www.youtube.com/*" //injects script into all urls
          //use match patterns
          //can also exclude
        ],
        "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
    
    
}

